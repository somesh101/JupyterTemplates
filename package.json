  "contributes": {
    "notebookProvider": [
      {
        "viewType": "jupyter-templates.viewType",
        "displayName": "Your Notebook Provider",
        "selector": [
          {
            "filenamePattern": "*.json"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "jupyter-templates.loadTemplate",
        "title": "Load Jupyter Template",
        "category": "JupyterLab Templates"
      },
      {
        "command": "jupyter-templates.createTemplate",
        "title": "Create Jupyter Template",
        "category": "JupyterLab Templates"
