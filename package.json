{
  "name": "jupyter-templates",
  "displayName": "jupyter-templates",
  "publisher": "somesh.km1@gmail.com",
  "license": "MIT",
  "git":"https://github.com/somesh101/JupyterTemplates.git",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "notebookProvider": [
            {
                "viewType": "jupyter-templates.viewType",
                "displayName": "Your Notebook Provider",
                "selector": [
                    {
                        "filenamePattern": "*.json"
                    }
                ]
            }
        ],
    "commands": [
      {
          "command": "jupyter-templates.loadTemplate1",
          "title": "Load Template 1"
      },
      {
          "command": "jupyter-templates.loadTemplate2",
          "title": "Load Template 2"
      },
      {
          "command": "jupyter-templates.createTemplate",
          "title": "Create Template"
      }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
